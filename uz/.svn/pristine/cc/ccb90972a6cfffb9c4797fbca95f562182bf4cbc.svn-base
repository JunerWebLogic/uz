html, body
{
	height: 100%;
}
body, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, form, fieldset, input, textarea, p, blockquote, th, td
{
	margin: 0px;
	padding: 0px;
}
body
{
	font: 14px/20px Microsoft YaHei, SimSun, Arial, Helvetica, sans-serif;
	text-align: center;
	color: #333;
	background:#fff;
}
input, select, button, textarea{
	vertical-align: middle;
}
ul li{
	list-style: none;
}
a{
	text-decoration: none;
	outline: none;
	color: #000;
}
img{
	border: 0;
	vertical-align: middle;
}
table{
	border-collapse: collapse;
	border-spacing: 0;
	text-align: left;
}

/*宽度和浮动类*/
.fl{
	float: left;
}
.fr{
	float: right;
}
.fn{
	float: none;
}
.tac{
	text-align: center;
}
.tal{
	text-align: left;
}
.tar{
	text-align: right;
}
