/* 弹窗样式 开始 ================== */
.aui_outer
{
	text-align: left;
}
table.aui_border, table.aui_dialog
{
	border:0;
	border-collapse: collapse;
	margin: 0;
	width: auto;
}
table.aui_dialog
{
	width: 100%;
}
.aui_state_noTitle table.aui_dialog
{
	width:auto;
}
.aui_nw, .aui_n, .aui_ne, .aui_w, .aui_c, .aui_e, .aui_sw, .aui_s, .aui_se, .aui_header, .aui_tdIcon, .aui_main, .aui_footer
{
	padding: 0;
}
.aui_state_focus .aui_dialog 
{
	box-shadow: 0 0 3px rgba(0, 0, 0, 0.4);
}
.aui_title
{
	height: 50px;
	font-size:18px;
	line-height: 50px;
	padding-left: 17px;
	color: #fff;
	background:url(../images/dialog_bg.jpg) repeat-x;
	overflow: hidden;
	text-overflow: ellipsis;
}
.aui_state_focus .aui_title
{
	color: #fff;
}
.aui_state_noTitle .aui_title
{
	display: none;
}
.aui_close, .aui_state_noTitle .aui_close
{
	position: absolute;  
	right: 13px;
	top: 18px;
	width: 17px;
	height:17px;
	display: inline-block;
	font:0/0 Arial;
	text-indent:-999px;
	overflow:hidden;
	background: url(../images/dialog_ico.png) no-repeat;
	z-index:20;
}
.aui_state_noTitle .aui_close
{
	top:15px;
	right:15px;
}
